	Doi = {10.1038/s41586-018-0510-7},
	Id = {Antoja2018},
	Isbn = {1476-4687},
	Journal = {Nature},
	Number = {7723},
	Pages = {360--362},
	Title = {A dynamically young and perturbed Milky Way disk},
